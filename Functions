#include <iostream>

using namespace std;

int CheckPrime(int a , int b){
    int count1=0,count2=0;
    for(int i=2; i<=a/2 ; i++){
        if(a%i==0)
            count1++;
    }
    for(int j=2;j<=b/2;j++){
        if(b%j==0)
            count2++;
    }
    if(count1==0 && count2==0)
        return 0;
    else
        return 1;
}
void SumOfPrime(int n){
    for(int i=n-2;i>=n/2;i--){
        int res=CheckPrime(i,n-i);
        if(res==0){
            cout<<"\n "<<n-i<<" + "<<i<<" = "<<n;
        }
    }
}

int main()
{
    int num=0;
    cout<<"Enter a positive integer : ";
    cin>>num;
    SumOfPrime(num);

    return 0;
}
